{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "ADB Compass",
  "version": "0.1.0",
  "identifier": "com.h1dr0n.adbcompass",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "ADB Compass",
        "width": 1000,
        "height": 700,
        "minWidth": 960,
        "minHeight": 600,
        "center": true,
        "decorations": true,
        "resizable": true,
        "dragDropEnabled": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "resources": {
      "binaries/*": "./",
      "resources/*": "./"
    },
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}